eateVNode","_component_MDBRow","class","_component_MDBCol","_component_MDBInput","type","label","id","$setup","placeholder","_ctx","outline","_component_MDBTable","captionTop","_hoisted_3","_hoisted_4","_Fragment","locataire","key","_hoisted_5","nom","prenom","email","_component_router_link","to","path","params"],"sourceRoot":"","sources":["C:\\Users\\Administrateur\\Desktop\\Nouveau dossier (3)\\ecf-localib\\src\\views\\Locataires.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <MDBBtn color=\"success\" v-on:click=\"show\" v-if=\"!isEditing\"\r\n      >Ajouter un locataire</MDBBtn\r\n    >\r\n    <MDBBtn color=\"danger\" v-on:click=\"hide\" v-else>Fermer le formulaire</MDBBtn>\r\n    <form v-if=\"isEditing\">\r\n      <!-- 2 column grid layout with text inputs for the first and last names -->\r\n      <MDBRow class=\"mb-4\">\r\n        <MDBCol>\r\n          <MDBInput type=\"text\" label=\"Nom\" id=\"nom\" v-model=\"nom\" />\r\n        </MDBCol>\r\n        <MDBCol>\r\n          <MDBInput type=\"text\" label=\"Prénom\" id=\"prenom\" v-model=\"prenom\" />\r\n        </MDBCol>\r\n      </MDBRow>\r\n      <MDBRow class=\"mb-4\">\r\n        <MDBCol>\r\n          <MDBInput type=\"email\" label=\"Adresse email\" id=\"email\" v-model=\"email\" />\r\n        </MDBCol>\r\n        <MDBCol>\r\n          <MDBInput type=\"password\" label=\"Password\" id=\"password\" v-model=\"password\" />\r\n        </MDBCol>\r\n      </MDBRow>\r\n      <!-- Submit button -->\r\n      <MDBBtn color=\"success\" @click=\"addLocataire()\">\r\n        Ajout d'un nouveau locataire\r\n      </MDBBtn>\r\n    </form>\r\n  </div>\r\n  <div v-if=\"!isEditing\">\r\n    <input\r\n      type=\"search\"\r\n      class=\"form-control\"\r\n      placeholder=\"Type query\"\r\n      aria-label=\"Search\"\r\n      v-model=\"search\"\r\n    />\r\n    <MDBBtn outline=\"primary\"> Recherche </MDBBtn>\r\n  </div>\r\n\r\n  <MDBTable captionTop v-if=\"!isEditing\">\r\n    <caption>\r\n      Tableau des locataires\r\n    </caption>\r\n    <thead>\r\n      <tr>\r\n        <th scope=\"col\">#locataire</th>\r\n        <th scope=\"col\">Nom</th>\r\n        <th scope=\"col\">Prénom</th>\r\n        <th scope=\"col\">Email</th>\r\n        <th></th>\r\n      </tr>\r\n    </thead>\r\n    <tbody>\r\n      <tr v-for=\"locataire of locataires\" :key=\"locataire.id\">\r\n        <th scope=\"row\">{{ locataire.id }}</th>\r\n        <td>{{ locataire.nom }}</td>\r\n        <td>{{ locataire.prenom }}</td>\r\n        <td>{{ locataire.email }}</td>\r\n        <td>\r\n          <MDBBtn color=\"warning\">\r\n            <!-- <router-link\r\n              :to=\"{\r\n                name: 'Locataire',\r\n                params: { id: locataire.id },\r\n              }\"\r\n            >\r\n              Modifier\r\n            </router-link> -->\r\n            <router-link :to=\"{ path: '/locataire/:id', params: { id: locataire.id } }\"\r\n              >Home</router-link\r\n            >\r\n          </MDBBtn>\r\n        </td>\r\n        <td>\r\n          <MDBBtn color=\"danger\" v-on:click=\"removeItem(locataire.id)\">Supprimer</MDBBtn>\r\n        </td>\r\n      </tr>\r\n    </tbody>\r\n  </MDBTable>\r\n</template>\r\n\r\n<script>\r\nimport { MDBRow, MDBCol, MDBInput, MDBBtn, MDBTable } from \"mdb-vue-ui-kit\";\r\nimport { ref } from \"vue\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  data() {\r\n    return { locataires: [], isEditing: false };\r\n  },\r\n  async created() {\r\n    try {\r\n      const res = await axios.get(`http://localhost:3000/locataires`);\r\n      this.locataires = res.data;\r\n    } catch (error) {\r\n      console.log(error);\r\n    }\r\n  },\r\n  methods: {\r\n    show() {\r\n      this.isEditing = true;\r\n    },\r\n    hide() {\r\n      this.isEditing = false;\r\n    },\r\n    async addLocataire() {\r\n      const res = await axios.post(`http://localhost:3000/locataires`, {\r\n        id: this.id,\r\n        nom: this.nom,\r\n        prenom: this.prenom,\r\n        email: this.email,\r\n      });\r\n      this.locataires = [...this.locataires, res.data];\r\n      this.nom = \"\";\r\n      this.prenom = \"\";\r\n      this.email = \"\";\r\n      this.isEditing = false;\r\n    },\r\n    removeItem(id) {\r\n      axios.delete(`http://localhost:3000/locataires/${id}`);\r\n      this.locataires = this.locataires.filter((locataire) => locataire.id !== id);\r\n    },\r\n  },\r\n  name: \"Locataires\",\r\n  components: {\r\n    MDBTable,\r\n    MDBRow,\r\n    MDBCol,\r\n    MDBInput,\r\n    MDBBtn,\r\n  },\r\n  setup() {\r\n    const nom = ref(\"\");\r\n    const prenom = ref(\"\");\r\n    const email = ref(\"\");\r\n    const password = ref(\"\");\r\n\r\n    return {\r\n      nom,\r\n      prenom,\r\n      email,\r\n      password,\r\n    };\r\n  },\r\n};\r\n</script>\r\n"]}]}